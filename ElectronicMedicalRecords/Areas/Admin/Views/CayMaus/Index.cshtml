        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ResultNuoiCay)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ImageNuoiCay)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ResultDinhDanh)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ImageDinhDanh)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NongDo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.InformationExamination.ID)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
