@model ElectronicMedicalRecords.Models.MultiplesModel

<a class="btn btn-primary" data-bs-toggle="collapse" href="#collapseCoXuongKhop" role="button" aria-expanded="false" aria-controls="collapseExample">
    Cơ Xương Khớp
</a>

<div class="collapse" id="collapseCoXuongKhop">
    <div class="card-body">
        <table class="table table-striped tableLoadClinical" id="tableCoXuongKhops" style="width:100%">
            <thead>
                <tr>
                    <th hidden></th>
                    <th hidden></th>
                    <th>Chỉ Định</th>
                    <th>Bệnh về Cơ Xương Khớp</th>
                </tr>
            </thead>
            <tbody>
                @for (int i = 0; i < Model.Detail_CoXuongKhops.Count(); i++)
                {
                    <tr>
                        <td hidden>
                            <input value="@Model.Detail_CoXuongKhops[i].InformationExamination_ID" type="text" name="Detail_CoXuongKhops[@i].InformationExamination_ID" hidden />
                            <input value="@Model.Detail_CoXuongKhops[i].CoXuongKhop_ID" type="text" name="Detail_CoXuongKhops[@i].CoXuongKhop_ID" hidden />
                        </td>
                        <td hidden>
                            <input value="@Model.Detail_CoXuongKhops[i].ID" type="text" name="Detail_CoXuongKhops[@i].ID" hidden />
                        </td>
                        <td>
                            <input hidden checked type="checkbox" value="true" class="form-check-input form-check-primary" data-id="@Model.Detail_CoXuongKhops[i].CoXuongKhop_ID" name="Detail_CoXuongKhops[@i].CoXuongKhop_ID">
                            <input disabled checked type="checkbox" value="true" class="form-check-input form-check-primary">
                        </td>
                        <td>
                            <label class="form-check-label">@Model.Detail_CoXuongKhops[i].CoXuongKhop.Name</label>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
