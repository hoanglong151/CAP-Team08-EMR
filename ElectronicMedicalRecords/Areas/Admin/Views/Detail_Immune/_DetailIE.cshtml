@model ElectronicMedicalRecords.Models.MultiplesModel

<div class="card-body">
    <a class="btn btn-primary" style="margin-bottom:12px" id="PrintResultMienDich">In kết quả xét nghiệm</a>
    <table class="table table-striped tableLoad" id="tableImmunes" style="width:100%">
        <thead>
            <tr>
                <th hidden></th>
                <th hidden></th>
                <th>Chỉ Định</th>
                <th>Tên Xét Nghiệm</th>
                <th>Kết Quả</th>
                <th>Chỉ Số Cân Bằng</th>
                <th>Đơn Vị</th>
            </tr>
        </thead>
        <tbody>
            @for (int i = 0; i < Model.Detail_Immunes.Count(); i++)
            {
                <tr>
                    <td hidden>
                        <input value="@Model.Detail_Immunes[i].InformationExamination_ID" type="text" name="Detail_Immunes[@i].InformationExamination_ID" hidden />
                        <input value="@Model.Detail_Immunes[i].Immue_ID" type="text" name="Detail_Immunes[@i].Immue_ID" hidden />
                    </td>
                    <td hidden>
                        @*<input value="@ViewBag.CTMau[i].ID" type="text" name="CTMau[@i].ID" hidden />*@
                        <input value="@Model.Detail_Immunes[i].ID" type="text" name="Detail_Immunes[@i].ID" hidden />
                    </td>
                    <td>
                        @*<input type="checkbox" value="false" class="form-check-input form-check-primary" name="CTMau[@i].ChiDinh" id="customColorCheck-@ViewBag.CTMau[i].ID" onchange="updateCountTests(@ViewBag.CTMau[i].ID, event)">*@
                        <input hidden checked type="checkbox" value="true" class="form-check-input form-check-primary" data-id="@Model.Detail_Immunes[i].ID" name="Detail_Immunes[@i].ID">
                        <input disabled checked type="checkbox" value="true" class="form-check-input form-check-primary">
                    </td>
                    <td>
                        <label class="form-check-label">@Model.Detail_Immunes[i].Immune.NameTest</label>
                    </td>
                    <td>
                        @Model.Detail_Immunes[i].Result
                        <input name="Detail_Immunes[@i].Result" value="@Model.Detail_Immunes[i].Result" hidden />
                    </td>
                    <td>
                        @Model.Detail_Immunes[i].Immune.CSBT
                    </td>
                    <td>
                        @Model.Detail_Immunes[i].Immune.Unit
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>

