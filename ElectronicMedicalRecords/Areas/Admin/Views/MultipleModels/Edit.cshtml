@section scripts{
    <script>
        var k = 1;
        var formatCss = document.querySelectorAll('.choices__list--single');
        formatCss.forEach((value, index) => {
            value.classList.add('form-control');
            value.style.paddingLeft = '12px';
        })
        var select = document.querySelector('#NameMedication');
        select.addEventListener('change', (e) => {
            var id = parseInt(e.target.value);
            $.ajax({
                type: "POST",
                url: "@Url.Action("FindMedication", "Medications")",
                data: { id: id},
                success: (res) => {
                    document.querySelector('#DVT').innerHTML = res.data.Unit;
                    document.querySelector('#IDMedication').setAttribute("value", res.data.ID);
                }
            })
        })

        function insRowMedication() {
            var NameMedication = $('#NameMedication').text();
            var UnitMedication = $('#UnitMedication').val();
            var NoteMedication = $('#NoteMedication').val();
            var IdMedication = $('#IDMedication').val();
            var medication = document.getElementById('TableMedication');
            var row = medication.insertRow(k);
            var cell0 = row.insertCell(0);
            var cell1 = row.insertCell(1);
            var cell2 = row.insertCell(2);
            var cell3 = row.insertCell(3);
            cell0.innerHTML = "<input type='number' id='medicationID' name='Prescription_Details" + '[' + (k - 1) + ']' + ".Medication_ID' value='" + IdMedication + "' hidden /> <input style='width:100%;border:none;pointer-events: none;' type='text' value='" + NameMedication + "' readonly/>";
            cell1.innerHTML = "<input style='width:100%;border:none;pointer-events: none;' type='number' id='numMedication' name='Prescription_Details" + '[' + (k - 1) + ']' + ".NumMedication' value='" + UnitMedication + "' readonly/>";
            cell2.innerHTML = "<input style='width:100%;border:none;pointer-events: none;' type='text' id='noteMedication' name='Prescription_Details" + '[' + (k - 1) + ']' + ".Note' value='" + NoteMedication + "' readonly/>";
            cell3.innerHTML = "<button class='btn btn-primary' id='Medication-" + k + "' type='button' onClick='deleteRow(" + k + ")'>Delete</button>";
            k++;
            }

        loadMedication();
        function loadMedication() {
            var InfoID = document.querySelector('#InformationExamination_ID').getAttribute('value');
            var medication = document.getElementById('TableMedication');
            $.ajax({
                type: "POST",
                data: { id: InfoID },
                url: "@Url.Action("LoadPrescription", "Prescription_Detail")",
                success: (res) => {
                    $.each(res.data, function (index, medicationByID) {
                        var row = medication.insertRow(k);
                        var cell0 = row.insertCell(0);
                        var cell1 = row.insertCell(1);
                        var cell2 = row.insertCell(2);
                        var cell3 = row.insertCell(3);
                        cell0.innerHTML = "<input type='number' id='infomartionID' name='Prescription_Details" + '[' + (k - 1) + ']' + ".InformationExamination_ID' value='" + medicationByID.InformationExamination_ID + "' hidden/> <input id='medicationID' type='number' name='Prescription_Details" + '[' + (k - 1) + ']' + ".Medication_ID' value='" + medicationByID.Medication_ID + "' hidden /> <input style='width:100%;border:none' type='text' value='" + medicationByID.Name + "' readonly/>";
                        cell1.innerHTML = "<input id='numMedication' style='width:100%;border:none' type='number' name='Prescription_Details" + '[' + (k - 1) + ']' + ".NumMedication' value='" + medicationByID.NumMedication + "' readonly/>";
                        cell2.innerHTML = "<input id='noteMedication' style='width:100%;border:none' type='text' name='Prescription_Details" + '[' + (k - 1) + ']' + ".Note' value='" + medicationByID.Note + "' readonly/>";
                        cell3.innerHTML = "<button class='btn btn-primary' id='Medication-" + k + "' type='button' onClick='deleteRow(" + k + ")'>Delete</button>";
                        k++;
                    })
                }
            })
        }

        function deleteRow(btn) {
            var index = $("#Medication-" + btn);
            index.parent().parent().remove();
            k--;
            updateRow();
        }

        function updateRow() {
            k = 1;
            var listTr = $('#tablePre tr');
            for (let i = 1; i < listTr.length; i++) {
                var InputNote = $(listTr[i]).children('td').children('input[id]');
                if (InputNote.length == 4) {
                    InputNote[0].setAttribute('name', `Prescription_Details[${k - 1}].InformationExamination_ID`);
                    InputNote[1].setAttribute('name', `Prescription_Details[${k - 1}].Medication_ID`);
                    InputNote[2].setAttribute('name', `Prescription_Details[${k - 1}].NumMedication`);
                    InputNote[3].setAttribute('name', `Prescription_Details[${k - 1}].Note`);
                } else {
                    InputNote[0].setAttribute('name', `Prescription_Details[${k - 1}].Medication_ID`);
                    InputNote[1].setAttribute('name', `Prescription_Details[${k - 1}].NumMedication`);
