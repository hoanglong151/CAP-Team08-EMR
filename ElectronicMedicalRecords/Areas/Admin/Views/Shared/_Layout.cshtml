                padding: 6px;
            }

        .noti-top-arrow {
            border-style: solid;
            border-width: 12px;
            border-color: transparent #000 transparent transparent;
            width: 24px;
            height: 24px;
        }

        .ul-noti::-webkit-scrollbar {
            width: 4px;
            border-radius: 4px;
        }

        .ul-noti::-webkit-scrollbar-thumb {
            border-radius: 4px;
            background: radial-gradient(circle, rgba(63,94,251,1) 0%, rgba(252,70,107,1) 100%);
        }

        .ul-noti::-webkit-scrollbar-track {
            -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3);
            background-color: #F5F5F5;
            border-radius: 4px;
        }

        span.noti {
            color: #eee;
            position: fixed;
            font-size: 18px;
            cursor: pointer;
            left: 16%;
            top: 10%;
            background-color: transparent;
            padding: 4px 8px;
