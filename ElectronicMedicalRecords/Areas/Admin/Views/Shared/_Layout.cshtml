    <style type="text/css">
        .noti-content {
            position: fixed;
            border-radius: 4px;
            top: 70px;
            width: 250px;
            display: none;
            left: 18%
        }

        ul#notiContent, ul#notiContentBS, ul#notiContentBS1 {
            max-height: 200px;
            overflow: auto;
            padding: 0px;
            margin: 0px;
            list-style: none;
            flex-grow: 1;
