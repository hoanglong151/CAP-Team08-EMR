    <script>
        $(document).ready(function () {
            $('.sidebar-item').find('[href="' + window.location.pathname + '"]').parent().addClass('active');
            $('.submenu-item.active').closest('.sidebar-item').addClass('active');
            $('.submenu-item.active').closest('.submenu').addClass('active');
        });
        var NumberInput = document.querySelectorAll('.numberInput');
        NumberInput.forEach((item, index) => {
            item.addEventListener("keypress", function (evt) {
                if (evt.which != 8 && evt.which != 0 && evt.which < 46 || evt.which == 47 || evt.which > 57) {
                    evt.preventDefault();
                }
            })
        })
    </script>

    <script type="text/javascript">
        $(function () {
            function getPatient() {
                $('#tableLoadPatient').DataTable({
                "ajax": {
                    "url": "@Url.Action("GetData", "Patients")",
                    "type": "GET",
                    "datatype": "json"
                },
                "columns": [
                    {
                        "data": "MaBN",
                    },
                    {
                        "data": "Name",
                    },
                    {
                        "data": "BirthDate",
                    },
                    {
                        "data": "Address",
                    },
                    {
                        "data": "Gender",
                    },
                    {
                        "className": "w85",
                        "data": null,
                        "render": function (data, type, row) {
                            var hrefCreateOldPatient = '@Url.Action("CreateOldPatient", "MultipleModels")/' + data.ID;
                            var hrefEditPatient = '@Url.Action("Edit","MultipleModels")/' + data.ID;
                            var hrefDetailPatient = '@Url.Action("Details","InformationExaminations")/' + data.ID;
                            var hrefConfirmBill = '@Url.Action("BillExamination", "MultipleModels")/' + data.ID;
                            var button = "<a class='btn btnHover print' style='margin:0 2px;' href='" + hrefConfirmBill + "'><i class='fas fa-print'></i></a><button class='btn btnHover plus' style='margin:0 2px;' onclick='createOldPatient(" + data.ID + ")' onclick='createOldPatient(" + data.ID + ")'><i class='fas fa-plus'></i></button><a class='btn btnHover edit' style='margin:0 2px;' href='" + hrefEditPatient + "'><i class='fas fa-user-edit'></i></a><a class='btn btnHover info' style='margin:0 2px;' href='" + hrefDetailPatient + "'><i class='fas fa-info'></i></a><button class='btn btnHover trash' style='margin:0 2px;' onclick='deletePatient(" + data.ID + ")'><i class='fas fa-trash'></i></button>";
                            return button;
                        },
                        "targets": 0,
                        "orderable": false
                    }
                ],
                "language": {
                    "emptyTable": "No data available",
                    "oPaginate": {
                        sNext: '<i class="fa fa-chevron-right"></i>',
                        sPrevious: '<i class="fa fa-chevron-left"></i>',
                    },
                    "info": "Hiển thị _START_/_END_ trên tổng _TOTAL_",
                    "lengthMenu": "Hiển thị _MENU_ người",
                    "search": "Tìm kiếm:",
                },
                searching: false,
            })
            }

            var notificationHubYTa = $.connection.notificationHubYTa;
            $.connection.hub.start().done(function () {
                getPatient();
                /*console.log("Notification hub started Y Tá");*/
            }).fail((e) => {
                alert(e)
            });

            notificationHubYTa.client.notifyPatient = function (message) {
                if (message && message.toLowerCase() == "added") {
                    $('#tableLoadPatient').DataTable().ajax.reload();
                }
            }
        })
    </script>

    <script type="text/javascript">
        $(function () {
            var RoleCheckBS = @(User.IsInRole("Bác Sĩ") ? "true" : "false");
            if (RoleCheckBS) {
                var count = 0;
                count = parseInt($('span.countBS').html()) || 0;
                if (count > 0) {
                    updateNotificationBS();
                }
                $('span.countBS', this).html("&nbsp;");
                $('html').click(function () {
                    $('.noti-content').hide();
                })

                /*updateNotificationBS();*/
                function updateNotificationBS() {
                    $('#notiContentBS').empty();
                    $('#notiContentBS').append($('<li>Loading...</li>'));

                    $.ajax({
                        type: "GET",
                        url: "@Url.Action("GetNotificationBS", "InformationExaminations")",
                        success: function (res) {
                            $('#notiContentBS').empty();
                            if (res.length == 0) {
                                $('#notiContentBS').append($('<li>No data available</li>'));
                            }
                            $.each(res.data, function (index, value) {
                                $('span.countBS').parent().children('i').addClass('bell');
                                var href = "@Url.Action("DetailsIE", "MultipleModels")" + "/" + value.ID;
                                $('#notiContentBS').append($('<li class="Notify"><a class="href-noti" href= ' + href + '>' + 'Bệnh nhân: ' + value.Name + '<p style="margin:0">Số lượng xét nghiệm: ' + value.count + '</p></a></li>'));
                            })
                            $('span.countBS').html(res.data.length);
                        }
                    })
                }
                var notificationHubBS = $.connection.notificationHubBS;
                $.connection.hub.start().done(function () {
                    updateNotificationBS();
                    /*console.log("Notification hub started Bác Sĩ");*/
                });
                notificationHubBS.client.notifyResult = function (message) {
                    if (message && message.toLowerCase() == "added") {
                        updateNotificationBS();
                    }
                }
            }
        })
    </script>
    <script type="text/javascript">
        $(function () {
            var RoleCheckKTV = @(User.IsInRole("Kỹ Thuật Viên") ? "true" : "false");
            if (RoleCheckKTV) {
            var count = 0;
            count = parseInt($('span.count').html()) || 0;
            if (count > 0) {
                updateNotification();
            }
            $('span.count', this).html("&nbsp;");
            $('html').click(function () {
                $('.noti-content').hide();
            })

          /*  updateNotification();*/
            function updateNotification() {
                $('#notiContent').empty();
                $('#notiContent').append($('<li>Loading...</li>'));
                $.ajax({
                    type: "GET",
                    url: "@Url.Action("GetNotification", "InformationExaminations")",
                    success: function (res) {
                        $('#notiContent').empty();
                        if (res.length == 0) {
                            $('#notiContent').append($('<li>No data available</li>'));
                        }
                        $.each(res.data, function (index, value) {
                            $('span.count').parent().children('i').addClass('bell');
