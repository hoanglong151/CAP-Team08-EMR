@model IEnumerable<ElectronicMedicalRecords.Models.MultiplesModel>
@{
    ViewBag.Title = "FindPatient";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    #sh-header-builder-9CcOom8qTf .sh-header-builder-main-container {
        background-color: #1c1c1c;
    }

    .pagePatient {
        padding: 150px 5rem;
    }

    .btn-search {
        padding: 16px;
        border-radius: 50%;
        border: none;
        background: #eee;
    }

    .btn-search:hover {
        color: red
    }

    #TableMedication td {
        padding: 0 !important;
    }
</style>

<section id="e-profile" class="pagePatient">
    <section class="section">
        <div class="card">
            <div class="card-header">
                Hồ Sơ Khám Bệnh
            </div>
            <div class="card-body">
                <form action="@Url.Action("SearchPatient","Home")" method="post">
                    <div class="form-search">
                        <div class="col-md-5">
                            <label>Mã Bệnh Nhân</label>
                            @if (ViewBag.CodePatient != null)
                            {
                                <input name="Code" type="text" class="form-control" value="@ViewBag.NamePatient" placeholder="Mã Bệnh Nhân" />
                            }
                            else
                            {
                                <input name="Code" type="text" class="form-control" placeholder="Mã Bệnh Nhân" />
                            }
                        </div>
                        <div class="col-md-1" style="text-align:center">
                            <h5>Hoặc</h5>
                        </div>
                        <div class="col-md-5">
                            <label>Mã BHYT</label>
                            @if (ViewBag.BHYTPatient != null)
                            {
                                <input name="BHYT" type="text" value="@ViewBag.BHYTPatient" class="form-control" placeholder="Mã BHYT" />
                            }
                            else
                            {
                                <input name="BHYT" type="text" class="form-control" placeholder="Mã BHYT" />
                            }
                        </div>
                        <div class="col-md-1" style="text-align:center">
                            <button type="submit" class="btn-search">
                                <i class="fas fa-search" style="font-size:18px"></i>
                            </button>
                        </div>
                    </div>
                </form>
                <h3>Lịch Tái Khám</h3>
                <table class="table table-striped" id="tableLoadPatient" style="width:100%">
                    <thead>
                        <tr>
                            <th class="mobile-CodePatient">Mã Bệnh Nhân</th>
                            <th class="mobile-CodeInsurance">Mã BHYT</th>
                            <th class="mobile-NamePatient">Họ Tên</th>
                            <th>Ngày Tài Khám</th>
                        </tr>
                    </thead>
                    <tbody>
                        @if (Model != null)
                        {
                            foreach (var info in Model)
                            {
                                if (DateTime.Today < info.InformationExamination.DateReExamination)
                                {
                                    <tr>
                                        <td class="mobile-CodePatient">@info.Patient.MaBN</td>
                                        <td class="mobile-CodeInsurance">@info.Patient.InsuranceCode</td>
                                        <td class="mobile-NamePatient">@info.Patient.Name</td>
                                        <td>@info.InformationExamination.DateReExamination.Value.ToString("dd/MM/yyyy")</td>
                                    </tr>
                                }
                            }
                        }
                    </tbody>
                </table>
                <h3>Hồ Sơ Khám Bệnh</h3>
                <table class="table table-striped" id="tableLoadPatient" style="width:100%">
                    <thead>
                        <tr>
                            <th class="mobile-CodePatient">Mã Bệnh Nhân</th>
                            <th class="mobile-CodeInsurance">Mã BHYT</th>
                            <th class="mobile-NamePatient">Họ Tên</th>
                            <th class="tab-YearPatient mobile-YearPatient">Năm Sinh</th>
                            <th>Ngày Khám</th>
                            <th class="tab-PatientSex mobile-PatientSex">Giới Tính</th>
                            <th class="actionPatientLoad">Chức Năng</th>
                        </tr>
                    </thead>
                    <tbody>
                        @if (Model != null)
                        {
                            foreach (var info in Model)
                            {
                                <tr>
                                    <td class="mobile-CodePatient">@info.Patient.MaBN</td>
                                    <td class="mobile-CodeInsurance">@info.Patient.InsuranceCode</td>
                                    <td class="mobile-NamePatient">@info.Patient.Name</td>
                                    <td class="tab-YearPatient mobile-YearPatient">@info.Patient.BirthDate.Value.ToString("yyyy")</td>
                                    <td>@info.InformationExamination.DateExamine</td>
                                    <td class="tab-PatientSex mobile-PatientSex">@info.Patient.Gender.Gender1</td>
                                    <td>
                                        <!-- Button trigger modal -->
                                        <button type="button" style="padding: 2px 8px;" class="btn btn-primary platform-pc" data-bs-toggle="modal" data-bs-target="#exampleModal-@info.InformationExamination.ID">
                                            Xem Chi Tiết
                                        </button>

                                        <button type="button" class="btn btn-primary platform-tab" data-bs-toggle="modal" data-bs-target="#exampleModal-@info.InformationExamination.ID">
                                            <i class="fas fa-info"></i>
                                        </button>

                                        <!-- Modal -->
                                        <div class="modal fade" id="exampleModal-@info.InformationExamination.ID" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
                                            <div class="modal-dialog" style="max-width:100%">
                                                <div class="modal-content">
                                                    <div class="modal-header">
                                                        <h5 class="modal-title" id="exampleModalLabel">Hồ Sơ Bệnh</h5>
                                                        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                                                    </div>
                                                    <div class="modal-body">
                                                        <div class="col-md-12">
                                                            <div class="card">
                                                                <div class="card-body">
                                                                    <ul class="nav nav-tabs" id="myTab" role="tablist">
                                                                        <li class="nav-item" role="presentation">
                                                                            <a class="nav-link active" id="home-tab" data-bs-toggle="tab" data-bs-target="#home"
                                                                               role="tab" aria-controls="home" aria-selected="true">Hồ Sơ</a>
                                                                        </li>
                                                                        @if (info.Detail_HistoryDiseases1.Count != 0 || info.Detail_HistoryDiseases2.Count != 0 || info.Detail_HistoryDiseases3.Count != 0
                                                                        || info.Detail_MedicalHistories.Count != 0)
                                                                        {
                                                                            <li class="nav-item" role="presentation">
                                                                                <a class="nav-link" id="history-tab" data-bs-toggle="tab" data-bs-target="#history"
                                                                                   role="tab" aria-controls="history" aria-selected="true">Tiền Sử, Bệnh Sử</a>
                                                                            </li>
                                                                        }
                                                                        @if (info.Detail_TuanHoans.Count != 0 || info.Detail_HoHaps.Count != 0 || info.Detail_TieuHoas.Count != 0 || info.Detail_ThanTietNieus.Count != 0
                                                                            || info.Detail_CoXuongKhops.Count != 0 || info.Detail_ThanKinhs.Count != 0 || info.Detail_TamThans.Count != 0 || info.Detail_NgoaiKhoas.Count != 0
                                                                            || info.Detail_SanPhuKhoas.Count != 0 || info.Detail_Mats.Count != 0 || info.Detail_TaiMuiHongs.Count != 0 || info.Detail_RangHamMats.Count != 0 || info.Detail_DaLieus.Count != 0 || info.Clinical != null)
                                                                        {
                                                                            <li class="nav-item" role="presentation">
                                                                                <a class="nav-link" id="home-tab" data-bs-toggle="tab" data-bs-target="#LamSang"
                                                                                   role="tab" aria-controls="home" aria-selected="true">Lâm Sàng</a>
                                                                            </li>
                                                                        }
                                                                        @if (info.Detail_CTMaus.Count != 0)
                                                                        {
                                                                            <li class="nav-item" role="presentation">
                                                                                <a class="nav-link" id="profile-tab" data-bs-toggle="tab" data-bs-target="#profile"
                                                                                   role="tab" aria-controls="profile" aria-selected="false">Công Thức Máu</a>
                                                                            </li>
                                                                        }
                                                                        @if (info.Detail_SinhHoaMaus.Count != 0)
                                                                        {
                                                                            <li class="nav-item" role="presentation">
                                                                                <a class="nav-link" id="profile-tab" data-bs-toggle="tab" data-bs-target="#SinhHoaMau"
                                                                                   role="tab" aria-controls="profile" aria-selected="false">Sinh Hóa Máu</a>
                                                                            </li>
                                                                        }
                                                                        @if (info.Detail_DongMaus.Count != 0)
                                                                        {
                                                                            <li class="nav-item" role="presentation">
                                                                                <a class="nav-link" id="profile-tab" data-bs-toggle="tab" data-bs-target="#DongMau"
                                                                                   role="tab" aria-controls="profile" aria-selected="false">Đông Máu</a>
                                                                            </li>
                                                                        }
                                                                        @if (info.Detail_NhomMaus.Count != 0)
                                                                        {
                                                                            <li class="nav-item" role="presentation">
                                                                                <a class="nav-link" id="profile-tab" data-bs-toggle="tab" data-bs-target="#NhomMau"
                                                                                   role="tab" aria-controls="profile" aria-selected="false">Nhóm Máu</a>
                                                                            </li>
                                                                        }
                                                                        @if (info.Detail_Urines.Count != 0)
                                                                        {
                                                                            <li class="nav-item" role="presentation">
                                                                                <a class="nav-link" id="profile-tab" data-bs-toggle="tab" data-bs-target="#urine"
                                                                                   role="tab" aria-controls="profile" aria-selected="false">Nước Tiểu</a>
                                                                            </li>
                                                                        }
                                                                        @if (info.Detail_Immunes.Count != 0)
                                                                        {
                                                                            <li class="nav-item" role="presentation">
                                                                                <a class="nav-link" id="profile-tab" data-bs-toggle="tab" data-bs-target="#immune"
                                                                                   role="tab" aria-controls="profile" aria-selected="false">Miễn Dịch</a>
                                                                            </li>
                                                                        }
                                                                        @if (info.Detail_Amniocentes.Count != 0)
                                                                        {
                                                                            <li class="nav-item" role="presentation">
                                                                                <a class="nav-link" id="profile-tab" data-bs-toggle="tab" data-bs-target="#amniocente"
                                                                                   role="tab" aria-controls="profile" aria-selected="false">Dịch Chọc Dò</a>
                                                                            </li>
                                                                        }
                                                                        @if (info.Detail_ViSinhs.Count != 0)
                                                                        {
                                                                            <li class="nav-item" role="presentation">
                                                                                <a class="nav-link" id="contact-tab" data-bs-toggle="tab" data-bs-target="#visinh"
                                                                                   role="tab" aria-controls="contact" aria-selected="false">Vi Sinh</a>
                                                                            </li>
                                                                        }
                                                                    </ul>
                                                                    <div class="tab-content" id="myTabContent">
                                                                        <div class="tab-pane fade show active" id="home" role="tabpanel"
                                                                             aria-labelledby="home-tab">
                                                                            <p class='my-2'>
                                                                                <section id="multiple-column-form">
                                                                                    <div class="row match-height">
                                                                                        <div class="col-12">
                                                                                            <div class="card">
                                                                                                <div class="card-header" style="padding-bottom:0px">
                                                                                                    <h4 class="card-title">Thông Tin Hành Chính</h4>
                                                                                                    <hr />
                                                                                                </div>
                                                                                                <div class="card-content">
                                                                                                    <div class="card-body">
                                                                                                        <div class="row">
                                                                                                            <div class="col-md-3 col-12">
                                                                                                                <div class="form-group">
                                                                                                                    <label for="first-name-column">Họ Và Tên</label>
                                                                                                                    <input value="@info.Patient.Name" class="form-control" readonly />
                                                                                                                </div>
                                                                                                            </div>
                                                                                                            <div class="col-md-3 col-12">
                                                                                                                <div class="form-group">
                                                                                                                    <label for="last-name-column">Giới Tính</label>
                                                                                                                    <input value="@info.Patient.Gender.Gender1" class="form-control" readonly />
                                                                                                                </div>
                                                                                                            </div>
                                                                                                            <div class="col-md-3 col-12">
                                                                                                                <div class="form-group">
                                                                                                                    <label for="city-column">Ngày Sinh</label>
                                                                                                                    <input class="form-control" type="date" name="Patient.BirthDate" value="@info.Patient.BirthDate.Value.ToString("yyyy-MM-dd")" id="birthdate" readonly />
                                                                                                                </div>
                                                                                                            </div>
                                                                                                            <div class="col-md-3 col-12">
                                                                                                                <div class="form-group">
                                                                                                                    <label for="country-floating">Dân Tộc</label>
                                                                                                                    <input value="@info.Patient.Nation1.Name" class="form-control" readonly />
                                                                                                                </div>
                                                                                                            </div>
                                                                                                            <div class="col-md-3 col-12">
                                                                                                                <div class="form-group">
                                                                                                                    <label for="email-id-column">Quốc Gia</label>
                                                                                                                    <input value="@info.Patient.Nation.Name" class="form-control" readonly />
                                                                                                                </div>
                                                                                                            </div>
                                                                                                            <div class="col-md-3 col-12">
                                                                                                                <div class="form-group">
                                                                                                                    <label for="email-id-column">Thành Phố/Tỉnh</label>
                                                                                                                    <input value="@info.Patient.HomeTown.HomeTown1" class="form-control" readonly />
                                                                                                                </div>
                                                                                                            </div>
                                                                                                            <div class="col-md-3 col-12">
                                                                                                                <div class="form-group">
                                                                                                                    <label for="company-column">Quận/Huyện</label>
                                                                                                                    <input value="@info.Patient.District.District1" class="form-control" readonly />
                                                                                                                </div>
                                                                                                            </div>
                                                                                                            <div class="col-md-3 col-12">
                                                                                                                <div class="form-group">
                                                                                                                    <label for="company-column">Phường/Xã</label>
                                                                                                                    <input value="@info.Patient.Ward.Ward1" class="form-control" readonly />
                                                                                                                </div>
                                                                                                            </div>
                                                                                                            <div class="col-md-2 col-12">
                                                                                                                <div class="form-group">
                                                                                                                    <label for="company-column">Số Nhà</label>
                                                                                                                    <input value="@info.Patient.NumHome" class="form-control" readonly />
                                                                                                                </div>
                                                                                                            </div>
                                                                                                            <div class="col-md-4 col-12">
                                                                                                                <div class="form-group">
                                                                                                                    <label for="company-column">Đường</label>
                                                                                                                    <input value="@info.Patient.Address" class="form-control" readonly />
                                                                                                                </div>
                                                                                                            </div>
                                                                                                            <div class="col-md-3 col-12">
                                                                                                                <div class="form-group">
                                                                                                                    <label for="email-id-column">Điện Thoại</label>
                                                                                                                    <input value="@("0" + info.Patient.Phone)" class="form-control" readonly />

                                                                                                                </div>
                                                                                                            </div>
                                                                                                            <div class="col-md-3 col-12">
                                                                                                                <div class="form-group">
                                                                                                                    <label for="email-id-column">Điện Thoại Zalo</label>
                                                                                                                    <input value="@("0" +info.Patient.PhoneZalo)" class="form-control" readonly />
                                                                                                                </div>
                                                                                                            </div>
                                                                                                            <div class="col-md-3 col-12">
                                                                                                                <div class="form-group">
                                                                                                                    <label for="email-id-column">Số BHYT</label>
                                                                                                                    <input value="@info.Patient.InsuranceCode" class="form-control" readonly />
                                                                                                                </div>
                                                                                                            </div>
                                                                                                        </div>
                                                                                                    </div>
                                                                                                </div>
                                                                                                <div class="card-header" style="padding-bottom:0px">
                                                                                                    <h4 class="card-title">Thông Tin Khám bệnh</h4>
                                                                                                    <hr />
                                                                                                </div>
                                                                                                <div class="card-content">
                                                                                                    <div class="card-body">
                                                                                                        <div class="row">
                                                                                                            <div class="col-md-4 col-12">
                                                                                                                <div class="form-group">
                                                                                                                    <label for="email-id-column">Ngày Đến</label>
                                                                                                                    <input name="InformationExamination.DateExamine" type="datetime" value="@info.InformationExamination.DateExamine" class="form-control" readonly />
                                                                                                                </div>
                                                                                                            </div>
                                                                                                            <div class="col-md-4 col-12">
                                                                                                                <div class="form-group">
                                                                                                                    <label for="email-id-column">Ngày Đi</label>
                                                                                                                    <input name="InformationExamination.DateEnd" type="datetime" value="@info.InformationExamination.DateEnd" class="form-control" readonly />
                                                                                                                </div>
                                                                                                            </div>
                                                                                                            <div class="col-md-4 col-12">
                                                                                                                <div class="form-group">
                                                                                                                    <label for="email-id-column">Bác Sĩ</label>
                                                                                                                    <input value="@info.InformationExamination.User_ID" name="InformationExamination.User_ID" hidden />
                                                                                                                    @if (info.InformationExamination.User != null)
                                                                                                                    {
                                                                                                                        <input value="@info.InformationExamination.User.Name" class="form-control" readonly />
                                                                                                                    }
                                                                                                                    else
                                                                                                                    {
                                                                                                                        <input value="" class="form-control" readonly />
                                                                                                                    }
                                                                                                                </div>
                                                                                                            </div>
                                                                                                            <div class="col-md-4 col-12">
                                                                                                                <div class="form-group">
                                                                                                                    <label for="email-id-column">Nhịp Tim</label>
                                                                                                                    <input value="@info.InformationExamination.HeartBeat" class="form-control" readonly />
                                                                                                                </div>
                                                                                                            </div>
                                                                                                            <div class="col-md-4 col-12">
                                                                                                                <div class="form-group">
                                                                                                                    <label for="email-id-column">Nhịp Thở</label>
                                                                                                                    <input value="@info.InformationExamination.Breathing" class="form-control" readonly />
                                                                                                                </div>
                                                                                                            </div>
                                                                                                            <div class="col-md-4 col-12">
                                                                                                                <div class="form-group">
                                                                                                                    <label for="email-id-column">Huyết Áp</label>
                                                                                                                    <input value="@info.InformationExamination.BloodPressure" class="form-control" readonly />
                                                                                                                </div>
                                                                                                            </div>
                                                                                                            <div class="col-md-4 col-12">
                                                                                                                <div class="form-group">
                                                                                                                    <label for="email-id-column">Cân Nặng</label>
                                                                                                                    <div class="input-group">
                                                                                                                        <input value="@info.InformationExamination.Weight" class="form-control" readonly />
                                                                                                                        <span class="input-group-text">kg</span>
                                                                                                                    </div>
                                                                                                                </div>
                                                                                                            </div>
                                                                                                            <div class="col-md-4 col-12">
                                                                                                                <div class="form-group">
                                                                                                                    <label for="email-id-column">Chiều Cao</label>
                                                                                                                    <div class="input-group">
                                                                                                                        <input value="@info.InformationExamination.Height" class="form-control" readonly />
                                                                                                                        <span class="input-group-text">cm</span>
                                                                                                                    </div>
                                                                                                                </div>
                                                                                                            </div>
                                                                                                            <div class="col-md-4 col-12">
                                                                                                                <div class="form-group">
                                                                                                                    <label for="email-id-column">Tình Trạng</label>
                                                                                                                    <input value="@info.InformationExamination.PatientStatu.Name" class="form-control" readonly />
                                                                                                                    <input value="@info.InformationExamination.PatientStatus_ID" name="InformationExamination.PatientStatus_ID" hidden />
                                                                                                                </div>
                                                                                                            </div>
                                                                                                        </div>
                                                                                                    </div>
                                                                                                </div>
                                                                                                <div class="card-header" style="padding-bottom:0px">
                                                                                                    <h4 class="card-title">Thông Tin Toa Thuốc</h4>
                                                                                                    <hr />
                                                                                                </div>

                                                                                                <div class="card-content">
                                                                                                    <div class="card-body">
                                                                                                        <div class="row">
                                                                                                            @if (info.Detail_DiagnosticsCategory != null)
                                                                                                            {
                                                                                                                if (info.Detail_DiagnosticsCategory.DiagnosticsCategory != null)
                                                                                                                {
                                                                                                                    <div class="col-md-12" style="margin-bottom:12px">
                                                                                                                        <label>Chẩn Đoán</label>
                                                                                                                        <input class="form-control" name="Detail_DiagnosticsCategory.Name" value="@info.Detail_DiagnosticsCategory.DiagnosticsCategory.Name" readonly />
                                                                                                                    </div>
                                                                                                                }
                                                                                                                if (info.Detail_DiagnosticsCategory.Advice != null)
                                                                                                                {
                                                                                                                    <div class="col-md-12" style="margin-bottom:12px">
                                                                                                                        <label>Lời Khuyên</label>
                                                                                                                        <input class="form-control" value="@info.Detail_DiagnosticsCategory.Advice" name="Detail_DiagnosticsCategory.Advice" readonly />
                                                                                                                    </div>
                                                                                                                }
                                                                                                            }
                                                                                                            <div class="col-md-12">
                                                                                                                <input id="InformationExamination_ID" value="@info.InformationExamination.ID" hidden />
                                                                                                                <table id="TableMedication" style="width:100%" border="1" class="table-bordered">
                                                                                                                    <tr>
                                                                                                                        <th>Tên Thuốc</th>
                                                                                                                        <th>Liều Lượng</th>
                                                                                                                        <th>Ghi Chú</th>
                                                                                                                    </tr>
                                                                                                                </table>
                                                                                                            </div>
                                                                                                        </div>
                                                                                                    </div>
                                                                                                </div>
                                                                                            </div>
                                                                                        </div>
                                                                                    </div>
                                                                                </section>
                                                                            </p>
                                                                        </div>
                                                                        <div class="tab-pane fade show" id="history" role="tabpanel"
                                                                             aria-labelledby="history-tab">
                                                                            <p class='my-2'>
                                                                                <section id="multiple-column-form">
                                                                                    <div class="row match-height">
                                                                                        <div class="col-12">
                                                                                            <div class="card">
                                                                                                @if (info.Detail_HistoryDiseases1.Count != 0 || info.Detail_HistoryDiseases2.Count != 0 || info.Detail_HistoryDiseases3.Count != 0)
                                                                                                {
                                                                                                    if (info.Detail_HistoryDiseases1.Count != 0 || info.Detail_HistoryDiseases2.Count != 0 || info.Detail_HistoryDiseases3.Count != 0)
                                                                                                    {
                                                                                                        <h2>Bệnh Tiền Sử</h2>
                                                                                                    }
                                                                                                    @*Ông/Bà*@
                                                                                                    if (info.Detail_HistoryDiseases1.Count != 0)
                                                                                                    {
                                                                                                        <div class="card-body">
                                                                                                            <h4>Ông/Bà</h4>
                                                                                                            <table class="table table-striped" id="tableHistoryDiseases1" style="width:100%">
                                                                                                                <thead>
                                                                                                                    <tr>
                                                                                                                        <th>Tiền sử bệnh</th>
                                                                                                                    </tr>
                                                                                                                </thead>
                                                                                                                <tbody>
                                                                                                                    @for (int i = 0; i < info.Detail_HistoryDiseases1.Count(); i++)
                                                                                                                    {
                                                                                                                        <tr>
                                                                                                                            <td>
                                                                                                                                @info.Detail_HistoryDiseases1[i].HistoryDisease.Name
                                                                                                                            </td>
                                                                                                                        </tr>
                                                                                                                    }
                                                                                                                </tbody>
                                                                                                            </table>
                                                                                                        </div>
                                                                                                    }

                                                                                                    @*Cha/Mẹ*@
                                                                                                    if (info.Detail_HistoryDiseases2.Count != 0)
                                                                                                    {
                                                                                                        <div class="card-body">
                                                                                                            <h4>Cha/Mẹ</h4>
                                                                                                            <table class="table table-striped" id="tableHistoryDiseases2" style="width:100%">
                                                                                                                <thead>
                                                                                                                    <tr>
                                                                                                                        <th>Tiền sử bệnh</th>
                                                                                                                    </tr>
                                                                                                                </thead>
                                                                                                                <tbody>
